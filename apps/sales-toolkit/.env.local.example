# OpenAI-compatible defaults for general use
OPENAI_API_KEY=sk-... # Optional if you pass x-api-key from the UI
OPENAI_BASE_URL=https://api.openai.com/v1
OPENAI_MODEL=gpt-4o-mini
OPENAI_EMBEDDINGS_MODEL=text-embedding-3-small

# xAI Grok for chat endpoints (AutoBrief, Battlecard, ICP, Objections ask)
XAI_API_KEY=sk-...
XAI_BASE_URL=https://api.x.ai/v1
XAI_MODEL=grok-code-fast-1

# OpenRouter (optional, if you want to use it elsewhere)
OPENROUTER_API_KEY=sk-or-...
OPENROUTER_MODEL=x-ai/grok-code-fast-1
